{
    "name": "brandon14/brandonclothier.me",
    "description": "A personal website for Brandon Clothier.",
    "version": "v1.6.0",
    "keywords": ["website", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "arcanedev/no-captcha": "^5.0",
        "fideloper/proxy": "^3.3",
        "filp/whoops": "^2.1",
        "htmlmin/htmlmin": "^5.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-cookie-consent": "^2.0",
        "spatie/laravel-sitemap": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "pragmarx/laravelcs": "^0.1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
