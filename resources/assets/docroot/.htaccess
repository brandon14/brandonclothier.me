# Block any bad bots set
# Apache < 2.4
<IfModule !mod_authz_core.c>
    <Limit GET POST>
        Order allow,deny
        Allow from all
        Deny from env=bad_bot
    </Limit>
</IfModule>
# Apache >= 2.4
<IfModule mod_authz_core.c>
    <Limit GET POST>
        <RequireAll>
            Require all granted
            Require not env bad_bot
        </RequireAll>
    </Limit>
</IfModule>

# Disable directory listing
Options All -Indexes

# Cache 1 Month for most static assets
<FilesMatch ".(jpg|jpeg|png|gif|ico|js|css|ttf|woff|svg)$">
    Header set Cache-Control "max-age=43800, public"
</FilesMatch>

# Prevent htaccess viewing
<Files .htaccess>
    order allow,deny
    deny from all
</Files>
